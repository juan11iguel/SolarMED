{
    "title": "Thermal storage",
    "subtitle": "model validation",
    "title_y": 0.95,
    "height": 800,
    "width": 1000,
    "margin": {
        "l": 20,
        "r": 20,
        "t": 100,
        "b": 20,
        "pad": 5
    },
    "vertical_spacing": 0.04,
    "xdomain": [
        0,
        1
    ],
    "legend_delta_y": 0.01,
    "plots": {
        "thermal_storage_flows": {
            "title": "<b>Flows</b>",
            "showlegend": true,
            "legend_position": "top",
            "row_height": 1,
            "bg_color": "bg_gray",
            "ylabels_left": [
                "m<sup>3</sup>/h"
            ],
            "ylims_left": "manual",
            "traces_left": [
                {
                    "var_id": "qts_dis",
                    "showlegend": true,
                    "name": "q<sub>ts,dis</sub>",
                    "mode": "lines",
                    "color": "plotly_blue",
                    "width": 3
                },
                {
                    "var_id": "qts_src",
                    "showlegend": true,
                    "name": "q<sub>ts,src</sub>",
                    "mode": "lines",
                    "color": "plotly_red",
                    "width": 3
                }
            ]
        },
        "thermal_storage_power_balance": {
            "row_height": 1,
            "title": "<b>Power balance</b>",
            "showlegend": true,
            "legend_position": "top",
            "bg_color": "bg_gray",
            "ylabels_left": [
                "kW<sub>th</sub>"
            ],
            "ylims_left": [
                0,
                260
            ],
            "traces_left": [
                {
                    "var_id": "Pth_ts_src",
                    "showlegend": true,
                    "name": "P<sub>th,ts,src</sub>",
                    "mode": "lines",
                    "color": "plotly_yellow",
                    "width": 3,
                    "fill_between": "Pth_ts_dis"
                },
                {
                    "var_id": "Pth_ts_dis",
                    "showlegend": true,
                    "name": "P<sub>th,ts,dis</sub>",
                    "mode": "lines",
                    "color": "wct_purple",
                    "width": 3,
                    "fill_between": "Pth_ts_src"
                }
            ]
        },
        "temperatures_hot_top": {
            "row_height": 1,
            "title": "Hot - top",
            "showlegend": true,
            "legend_position": "top",
            "bg_color": "bg_blue",
            "ylabels_left": [
                "\u2070C"
            ],
            "ylims_left": [
                60,
                110
            ],
            "tight_vertical_spacing": true,
            "traces_left": [
                {
                    "var_id": "Tts_h_t",
                    "showlegend": true,
                    "mode": "lines",
                    "color": "plotly_red",
                    "width": 3
                },
                {
                    "var_id": "Tts_h_in",
                    "showlegend": true,
                    "mode": "markers",
                    "color": "plotly_yellow",
                    "width": 3,
                    "opacity": 0.5,
                    "conditional": {
                        "var_id": "qts_src",
                        "showlegend": true,
                        "operator": ">",
                        "threshold_value": 1
                    }
                },
                {
                    "var_id": "Tts_h_out",
                    "showlegend": true,
                    "mode": "markers",
                    "color": "wct_purple",
                    "width": 3,
                    "opacity": 0.5,
                    "conditional": {
                        "var_id": "qts_dis",
                        "showlegend": true,
                        "operator": ">",
                        "threshold_value": 1
                    }
                }
            ]
        },
        "temperatures_hot_med": {
            "row_height": 1,
            "title": "Hot - medium",
            "showlegend": false,
            "legend_position": "top",
            "bg_color": "bg_blue",
            "ylabels_left": [
                "\u2070C"
            ],
            "ylims_left": "manual",
            "tight_vertical_spacing": true,
            "traces_left": [
                {
                    "var_id": "Tts_h_m",
                    "showlegend": false,
                    "mode": "lines",
                    "color": "plotly_red",
                    "width": 3
                }
            ]
        },
        "temperatures_hot_bottom": {
            "row_height": 1,
            "title": "Hot - bottom",
            "showlegend": true,
            "legend_position": "top",
            "bg_color": "bg_blue",
            "ylabels_left": [
                "\u2070C"
            ],
            "ylims_left": "manual",
            "traces_left": [
                {
                    "var_id": "Tts_h_b",
                    "showlegend": false,
                    "mode": "lines",
                    "color": "plotly_red",
                    "width": 3
                }
            ]
        },
        "temperatures_cold_top": {
            "row_height": 1,
            "title": "Cold - top",
            "showlegend": true,
            "legend_position": "top",
            "bg_color": "bg_blue",
            "ylabels_left": [
                "\u2070C"
            ],
            "ylims_left": "manual",
            "tight_vertical_spacing": true,
            "traces_left": [
                {
                    "var_id": "Tts_c_t",
                    "showlegend": true,
                    "mode": "lines",
                    "color": "plotly_blue",
                    "width": 3
                }
            ]
        },
        "temperatures_cold_med": {
            "row_height": 1,
            "title": "Cold - Medium",
            "showlegend": true,
            "legend_position": "top",
            "bg_color": "bg_blue",
            "ylabels_left": [
                "\u2070C"
            ],
            "ylims_left": "manual",
            "tight_vertical_spacing": true,
            "traces_left": [
                {
                    "var_id": "Tts_c_m",
                    "showlegend": true,
                    "mode": "lines",
                    "color": "plotly_blue",
                    "width": 3
                }
            ]
        },
        "temperatures_cold_bottom": {
            "row_height": 1,
            "title": "Cold - bottom",
            "showlegend": true,
            "legend_position": "top",
            "bg_color": "bg_blue",
            "ylabels_left": [
                "\u2070C"
            ],
            "ylims_left": "manual",
            "traces_left": [
                {
                    "var_id": "Tts_c_b",
                    "showlegend": true,
                    "mode": "lines",
                    "color": "plotly_blue",
                    "width": 3
                },
                {
                    "var_id": "Tts_c_in",
                    "showlegend": true,
                    "mode": "markers",
                    "color": "wct_purple",
                    "width": 3,
                    "opacity": 0.5,
                    "conditional": {
                        "var_id": "qts_dis",
                        "showlegend": true,
                        "operator": ">",
                        "threshold_value": 1
                    }
                }
            ]
        }
    }
}
