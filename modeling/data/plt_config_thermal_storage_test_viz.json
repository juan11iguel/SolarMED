{
    "title": "Thermal storage",
    "subtitle": "experimental data visualization",
    "height": 600,
    "width": 1000,
    "title_y": 0.95,
    "margin": {
        "l": 20,
        "r": 20,
        "t": 100,
        "b": 20,
        "pad": 5
    },
    "vertical_spacing": 0.08,
    "xdomain": [
        0,
        0.95
    ],
    "plots": {
        "thermal_storage_flows": {
            "title": "<b>Flows</b>",
            "showlegend": true,
            "row_height": 0.6,
            "bg_color": "bg_gray",
            "ylabels_left": [
                "m<sup>3</sup>/h"
            ],
            "ylims_left": "manual",
            "traces_left": [
                {
                    "var_id": "qts_dis",
                    "showlegend": true,
                    "name": "q<sub>ts,dis</sub>",
                    "mode": "lines",
                    "color": "plotly_blue",
                    "width": 3
                },
                {
                    "var_id": "qts_dis_original",
                    "showlegend": true,
                    "name": "q<sub>ts,dis,measured</sub>",
                    "color": "plotly_blue",
                    "width": 1.5,
                    "dash": "dash"
                },
                {
                    "var_id": "qts_src",
                    "showlegend": true,
                    "name": "q<sub>ts,src</sub>",
                    "mode": "lines",
                    "color": "plotly_red",
                    "width": 3
                },
                {
                    "var_id": "qts_src_original",
                    "showlegend": true,
                    "name": "q<sub>ts,src,measured</sub>",
                    "mode": "lines",
                    "color": "plotly_red",
                    "dash": "dash",
                    "width": 1.5
                }
            ]
        },
        "thermal_storage_power_balance": {
            "row_height": 0.6,
            "title": "<b>Power balance</b>",
            "showlegend": true,
            "bg_color": "bg_gray",
            "ylabels_left": [
                "kW<sub>th</sub>"
            ],
            "ylims_left": [
                0,
                260
            ],
            "traces_left": [
                {
                    "var_id": "Pth_ts_src",
                    "showlegend": true,
                    "name": "P<sub>ts,src</sub>",
                    "mode": "lines",
                    "color": "plotly_yellow",
                    "width": 3,
                    "fill_between": "Pth_ts_dis"
                },
                {
                    "var_id": "Pth_ts_dis",
                    "showlegend": true,
                    "name": "P<sub>ts,dis</sub>",
                    "mode": "lines",
                    "color": "wct_purple",
                    "width": 3,
                    "fill_between": "Pth_ts_src"
                }
            ]
        },
        "thermal_storage_temperatures": {
            "row_height": 1,
            "title": "<b>Hot tank</b>",
            "showlegend": true,
            "bg_color": "bg_blue",
            "ylabels_left": [
                "\u2070C"
            ],
            "ylims_left": [
                60,
                110
            ],
            "tight_vertical_spacing": true,
            "traces_left": [
                {
                    "var_id": "Tts_h_t",
                    "showlegend": true,
                    "mode": "lines",
                    "color": "plotly_red",
                    "width": 3
                },
                {
                    "var_id": "Tts_h_m",
                    "showlegend": true,
                    "mode": "lines",
                    "dash": "dash",
                    "color": "plotly_red",
                    "width": 3
                },
                {
                    "var_id": "Tts_h_b",
                    "showlegend": true,
                    "mode": "lines",
                    "color": "plotly_red",
                    "dash": "dot",
                    "width": 3
                },
                {
                    "var_id": "Tts_h_in",
                    "showlegend": true,
                    "mode": "markers",
                    "color": "plotly_yellow",
                    "width": 3,
                    "opacity": 0.5,
                    "conditional": {
                        "var_id": "qts_src",
                        "showlegend": true,
                        "operator": ">",
                        "threshold_value": 1
                    }
                },
                {
                    "var_id": "Tts_h_out",
                    "showlegend": true,
                    "mode": "markers",
                    "color": "wct_purple",
                    "width": 3,
                    "opacity": 0.5,
                    "conditional": {
                        "var_id": "qts_dis",
                        "showlegend": true,
                        "operator": ">",
                        "threshold_value": 1
                    }
                }
            ]
        },
        "thermal_storage_temperatures_cold": {
            "row_height": 1,
            "title": "<b>Cold tank</b>",
            "showlegend": true,
            "bg_color": "bg_blue",
            "ylabels_left": [
                "\u2070C"
            ],
            "ylims_left": [
                60,
                100
            ],
            "traces_left": [
                {
                    "var_id": "Tts_c_t",
                    "showlegend": true,
                    "mode": "lines",
                    "color": "plotly_green",
                    "width": 3
                },
                {
                    "var_id": "Tts_c_m",
                    "showlegend": true,
                    "mode": "lines",
                    "dash": "dash",
                    "color": "plotly_green",
                    "width": 3
                },
                {
                    "var_id": "Tts_c_b",
                    "showlegend": true,
                    "mode": "lines",
                    "color": "plotly_green",
                    "dash": "dot",
                    "width": 3
                },
                {
                    "var_id": "Tts_c_in",
                    "showlegend": true,
                    "mode": "markers",
                    "color": "c_blue",
                    "width": 3,
                    "opacity": 0.5,
                    "conditional": {
                        "var_id": "qts_dis",
                        "showlegend": true,
                        "operator": ">",
                        "threshold_value": 1
                    }
                }
            ]
        }
    }
}